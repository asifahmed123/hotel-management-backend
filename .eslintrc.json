{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": ["google"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint"],
  "rules": {
    "indent": "off",
    "@typescript-eslint/indent": ["error", 2],
    "no-console": "warn",
    "new-cap": "off",
    "require-jsdoc": "off"
  }
}
